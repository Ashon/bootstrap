#!/bin/bash
# This is the default theme for gitprompt.sh
override_git_prompt_colors() {
  GIT_PROMPT_THEME_NAME="Jwl"

  GIT_PROMPT_PREFIX=" ["
  GIT_PROMPT_SUFFIX="]"
  GIT_PROMPT_SEPARATOR=""
  GIT_PROMPT_BRANCH="${Magenta}"
  GIT_PROMPT_STAGED=" ${DimRed}staged-"
  GIT_PROMPT_CONFLICTS=" ${DimRed}conflicts-"
  GIT_PROMPT_CHANGED=" ${Blue}changed-"
  GIT_PROMPT_REMOTE=" "
  GIT_PROMPT_UNTRACKED=" ${Cyan}untracked-"
  GIT_PROMPT_STASHED=" ${Blue}stashed-"
  GIT_PROMPT_CLEAN=" ${DimGreen}clean"

  GIT_PROMPT_COMMAND_OK="${DimGreen}OK${ResetColor}"
  GIT_PROMPT_COMMAND_FAIL="${DimRed}FAIL-_LAST_COMMAND_STATE_${ResetColor}"
  GIT_PROMPT_VIRTUALENV="[${Blue}_VIRTUALENV_${ResetColor}] "

  GIT_PROMPT_START_USER="\n* ${DimCyan}\u@\h${ResetColor}: ${Yellow}${PathShort}${ResetColor}"
  GIT_PROMPT_END_USER="\n* \D{%F %T} _LAST_COMMAND_INDICATOR_ $ "

  GIT_PROMPT_START_ROOT="${GIT_PROMPT_START_USER}"
  GIT_PROMPT_END_ROOT="${ResetColor}\n>[\D{%F %T}] # "
  GIT_PROMPT_SYMBOLS_AHEAD="ahead:"
  GIT_PROMPT_SYMBOLS_BEHIND="behind:"
  GIT_PROMPT_SYMBOLS_PREHASH=":"
  GIT_PROMPT_SYMBOLS_NO_REMOTE_TRACKING="L"
}

reload_git_prompt_colors "Jwl"
